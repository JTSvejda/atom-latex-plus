{
  "name": "latex-plus",
  "main": "./lib/main",
  "version": "0.9.1",
  "description": "LaTeX for Atom.",
  "keywords": [
    "tex",
    "latex",
    "pdflatex",
    "xelatex",
    "lualatex",
    "texlive",
    "miktex"
  ],
  "repository": "https://github.com/andrewrynhard/atom-latex-plus",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "devDependencies": {
    "babel": "5.8.x",
    "eslint": "^1.0.0",
    "babel-eslint": "^4.0.5"
  },
  "dependencies": {
    "babel-core": "5.8.x",
    "babel-runtime": "5.8.x",
    "atom-message-panel": "latest",
    "atom-package-deps": "^4.0.1"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "activationHooks": [
    "language-latex:grammar-used"
  ],
  "package-deps": [
    "pdf-view"
  ]
}
